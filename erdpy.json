{
    "configurations": {
        "default": {
            "proxy": "https://api.elrond.com",
            "chainID": "1"
        }
    },
    "contract":{
        "deployledger":{
            "verbose": true,
            "bytecode": "output/elrond-bulk.wasm",
            "recall-nonce": true,
            "ledger": true,
            "ledger-address-index": 3,
            "gas-limit": 59999999,
            "send": true,
            "outfile": "deploy-testnet.interaction.json"
        },
        "deploy":{
            "verbose": true,
            "bytecode": "output/elrond-bulk.wasm",
            "recall-nonce": true,
            "pem": "wallet-owner.pem",
            "gas-limit": 59999999,
            "send": true,
            "outfile": "deploy-testnet.interaction.json"
        }
     }
}
